#include <iostream>
using namespace std;
int main(){
    int L,R,apper=0;
    cin>>L>>R;
    for(int i=L;i<=R;i++){
        int k=i;
        while(k>10){
           if(k%10==2){
                apper++;
            }
            k/=10;
        }
        if(k==2)apper++;
    }
    cout<<apper<<endl;
    return 0;
}
